<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.wanderlost" version="1.0.0" versionCode="" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="AutoHideSplashScreen" value="true" />
